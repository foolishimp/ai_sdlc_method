{
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "\"$CLAUDE_PROJECT_DIR\"/imp_claude/code/.claude-plugin/plugins/genisis/hooks/on-session-start.sh",
            "timeout": 10
          }
        ]
      }
    ],
    "UserPromptSubmit": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "\"$CLAUDE_PROJECT_DIR\"/imp_claude/code/.claude-plugin/plugins/genisis/hooks/on-iterate-start.sh",
            "timeout": 5
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "Edit|Write",
        "hooks": [
          {
            "type": "command",
            "command": "\"$CLAUDE_PROJECT_DIR\"/imp_claude/code/.claude-plugin/plugins/genisis/hooks/on-artifact-written.sh",
            "timeout": 5
          }
        ]
      }
    ],
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "\"$CLAUDE_PROJECT_DIR\"/imp_claude/code/.claude-plugin/plugins/genisis/hooks/on-stop-check-protocol.sh",
            "timeout": 10
          }
        ]
      }
    ]
  }
}
